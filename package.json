{
  "name": "universal-deepify",
  "version": "1.0.0",
  "description": "Will get/set properties and it can also execute functions nested deep in a object",
  "main": "lib/deepify",
  "scripts": {
    "build": "webpack --output-library deepify --output-library-target umd --optimize-minimize",
    "test": "mocha tests/**.spec.* -r chai/register-expect --require babel-core/register"
  },
  "keywords": [
    "deep",
    "object",
    "getter",
    "setter",
    "universal",
    "browser",
    "node"
  ],
  "author": "Ricardo Ibarra <coder10@gmail.com> (http://doitfy.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^5.0.5",
    "mocha-lcov-reporter": "^1.3.0",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13"
  }
}
